{
    "Version": "2012-10-17",
    "Id": "AccessControl",
    "Statement": [
        {
            "Sid": "AllowSSLRequestsOnly",
            "Effect": "Deny",
            "Principal": "*",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::cdk-hnb659fds-assets-122610492430-us-east-1",
                "arn:aws:s3:::cdk-hnb659fds-assets-122610492430-us-east-1/*"
            ],
            "Condition": {
                "Bool": {
                    "aws:SecureTransport": "false"
                }
            }
        },
        {
            "Sid": "AllowCDKDeployment",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::122610492430:role/cdk-hnb659fds-deploy-role-122610492430-us-east-1"
            },
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::cdk-hnb659fds-assets-122610492430-us-east-1",
                "arn:aws:s3:::cdk-hnb659fds-assets-122610492430-us-east-1/*"
            ]
        }
    ]
}
